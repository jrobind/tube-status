{
  "name": "tube-status",
  "version": "1.0.0",
  "description": "PWA providing current London tube line status",
  "main": "server/index.js",
  "scripts": {
    "start": "NODE_ENV=dev DEBUG=app:* nodemon server/index.js",
    "build": "webpack --config webpack.config.js",
    "test": "jest --env=jest-environment-jsdom-sixteen --coverage",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "eslint . --ext .js",
    "watch": "npm run build -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jrobind/tube-status.git"
  },
  "keywords": [
    "tube",
    "status",
    "updates",
    "london",
    "underground"
  ],
  "author": "James Robinson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jrobind/tube-status/issues"
  },
  "homepage": "https://github.com/jrobind/tube-status#readme",
  "jest": {
    "verbose": true,
    "testEnvironment": "jest-environment-jsdom-sixteen",
    "setupFiles": [
      "jest-localstorage-mock",
      "<rootDir>/jest/setup.js"
    ]
  },
  "dependencies": {
    "abort-controller": "^3.0.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "config": "^3.2.2",
    "cron": "^1.7.2",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.9",
    "node-fetch": "^2.6.0",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "socket.io": "^2.3.0",
    "web-push": "^3.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.10.1",
    "@types/jest": "^25.2.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-builtin-classes": "^0.6.1",
    "coveralls": "^3.1.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.7.2",
    "eslint-config-google": "^0.14.0",
    "jest": "^26.0.1",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jest-localstorage-mock": "^2.4.2",
    "mini-css-extract-plugin": "^0.9.0",
    "nodemon": "^1.19.4",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "wait-for-expect": "^3.0.2",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-fix-style-only-entries": "^0.4.0"
  }
}
